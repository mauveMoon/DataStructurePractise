#include<iostream>
#include<vector>

using namespace std;

class UnionFind{
public:
	vector<int> *father;
	UnionFind(int n){
		father = new vector<int>(n, -1);
	}
	~UnionFind(){
		delete father;
	}
	void union_root(int v1, int v2){
		int root1 = father[v1], root2 = father[v2];
	}
};

int main(){
	return 0;
}
